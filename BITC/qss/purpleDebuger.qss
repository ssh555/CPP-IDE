QPushButton{
    background-color: #C5D3ED;/*未按下时的背景颜色*/
    background-origin: border;
    border-width: 10px;
    font:white 12px;
}
QPushButton:pressed{
    background-origion: content;
    border-color: gray;
    background-color: qradialgradient(cx:0.5, cy:0.5, radius: 2.5,
    fx:0.5, fy:0.5, stop:0 #C5D3ED, stop:1 #7F9ED7);/*按下时的背景颜色*/
}

