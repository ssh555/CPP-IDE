QPushButton{
    background-color: #434343;/*未按下时的背景颜色*/
    background-origin: border;
    border-width: 10px;
    font:white 12px;
    color: #EBF1FF;
}
QPushButton:pressed{
    background-origion: content;
    border-color: gray;
    background-color: qradialgradient(cx:0.5, cy:0.5, radius: 2.5,
    fx:0.5, fy:0.5, stop:0 #434343, stop:1 #EAEAEA);/*按下时的背景颜色*/
}

